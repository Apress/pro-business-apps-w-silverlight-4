<navigation:Page x:Class="Chapter14Sample.Views.COM" 
           xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
           xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
           xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
           xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
           mc:Ignorable="d"
           xmlns:navigation="clr-namespace:System.Windows.Controls;assembly=System.Windows.Controls.Navigation"
           d:DesignWidth="640" d:DesignHeight="480"
           Title="COM Page">
    <Grid x:Name="LayoutRoot">
        <StackPanel>
            <Button Name="StartExeButton" Content="Start Exe" Height="40" Width="120" Margin="5" Click="StartExeButton_Click" />
            <Button Name="CreateEmailButton" Content="Create Email" Height="40" Width="120" Margin="5" Click="CreateEmailButton_Click" />
            <Button Name="OpenWordButton" Content="Open Word" Height="40" Width="120" Margin="5" Click="OpenWordButton_Click" />
            <Button Name="OpenExcelButton" Content="Open Excel" Height="40" Width="120" Margin="5" Click="OpenExcelButton_Click" />
        </StackPanel>
    </Grid>
</navigation:Page>
