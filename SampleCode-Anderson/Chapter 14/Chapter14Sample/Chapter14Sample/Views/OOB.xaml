<navigation:Page x:Class="Chapter14Sample.Views.OOB" 
           xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
           xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
           xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
           xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
           mc:Ignorable="d"
           xmlns:navigation="clr-namespace:System.Windows.Controls;assembly=System.Windows.Controls.Navigation"
           d:DesignWidth="640" d:DesignHeight="480"
           Title="Install Page">
    <Grid x:Name="LayoutRoot">
        <StackPanel>
            <Button Content="Install" Height="39" Name="InstallButton" Margin="5" Width="150" Click="InstallButton_Click" />
            <Button Content="Toast" Height="39" Name="ToastButton" Margin="5" Width="150" Click="ToastButton_Click" />
            <Button Content="Check for Update" Height="39" Name="CheckUpdateButton" Margin="5" Width="150" Click="CheckUpdateButton_Click" />
        </StackPanel>
    </Grid>
</navigation:Page>
